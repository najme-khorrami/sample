<script>
    $(document).ready(function () {
    var slider = $('.owl-carousel-slider');
    slider.owlCarousel({
        rtl:true,
        loop:true,
        items:1,
        nav:false,
        dots:true,
        autoplay:true,
        autoplayTimeout:4000, 
    })
    $('.slider-parent .slider-dots').click(function () {
        slider.trigger('next.owl.carousel', [900]);
    });
    })
</script> 
